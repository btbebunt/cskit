{
  "name": "cskit-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "dev:stable": "npm run clean && next dev --turbo",
    "build": "next build",
    "build:clean": "npm run clean && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next && rimraf node_modules/.cache && npm cache clean --force",
    "clean:full": "rimraf .next && rimraf node_modules && rimraf package-lock.json && npm cache clean --force && npm install",
    "reset": "npm run clean:full && npm run dev"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.0"
  }
}
